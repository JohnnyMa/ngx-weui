{
  "name": "ngx-weui",
  "version": "7.0.0",
  "description": "weui for angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cipchk/ngx-weui.git"
  },
  "keywords": [
    "ngx-weui",
    "ng2-weui",
    "ngx weui",
    "ng2 weui",
    "angular weui",
    "angular2 weui",
    "angular4 weui",
    "weui"
  ],
  "author": "cipchk <cipchk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cipchk/ngx-weui/issues"
  },
  "homepage": "https://github.com/cipchk/ngx-weui#readme",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json 'components/**/*.ts'",
    "test": "ng test --watch=false --code-coverage",
    "site": "npm run site:init && ng serve --port 0 --open",
    "site:init": "node scripts/site/main init",
    "site:build": "npm run site:init && ng build --prod --build-optimizer --base-href /ngx-weui/",
    "site:build:netlify": "npm run site:init && ng build --prod --build-optimizer --base-href /",
    "site:gh": "npm run site:build && gh-pages -d ./dist",
    "build": "bash scripts/build/build.sh",
    "release": "npm run build && cd publish && npm publish --access public"
  },
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "weui": "^1.1.3",
    "angular-qq-maps": "^7.0.0",
    "font-awesome": "^4.7.0",
    "ngx-countdown": "^3.1.0",
    "ngx-gesture-password": "^7.0.0",
    "ngx-toastr": "^9.1.1",
    "swiper": "^4.4.2",
    "g2-mobile": "^2.2.0-beta.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.1",
    "@angular/cli": "~7.1.1",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/sinon": "^2.3.1",
    "codecov": "^3.1.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "ng-packagr": "^4.4.5",
    "tsickle": "^0.34.0",
    "mark-twain": "^2.0.3",
    "mustache": "^3.0.1",
    "less-plugin-clean-css": "^1.5.1",
    "fs-extra": "^7.0.1",
    "sinon": "^4.4.2"
  }
}